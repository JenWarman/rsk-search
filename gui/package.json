{
  "name": "gui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development",
    "start-prod": "ng serve --proxy-config=src/proxy.conf.prod.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-api-client": "rm -rf ./src/app/lib/api-client && api-client-generator -s ./src/assets/swagger.json -o ./src/app/lib/api-client -t all",
    "docker-build": "DOCKER_IMAGE_NAME=rsk-search-gui ../deploy/script/docker-build.sh",
    "docker-run": "docker run -it -p 8080:80 warmans/rsk-search-gui:latest",
    "docker-publish": "DOCKER_IMAGE_NAME=rsk-search-gui ../deploy/script/docker-publish.sh",
    "release": "npm run build && npm run docker-build && npm run docker-publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.7",
    "@angular/common": "15.2.7",
    "@angular/compiler": "15.2.7",
    "@angular/core": "15.2.7",
    "@angular/forms": "15.2.7",
    "@angular/localize": "15.2.7",
    "@angular/platform-browser": "15.2.7",
    "@angular/platform-browser-dynamic": "15.2.7",
    "@angular/router": "15.2.7",
    "@ng-bootstrap/ng-bootstrap": "^14.1.0",
    "@popperjs/core": "^2.11.6",
    "@sentry/angular-ivy": "^7.47.0",
    "@types/w3c-web-hid": "^1.0.3",
    "@types/w3c-web-usb": "^1.0.6",
    "animate.css": "^4.1.1",
    "api-client-generator": "^4.7.1",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.9.1",
    "chart.js": "^3.9.1",
    "date-fns": "^2.29.3",
    "diff2html": "^3.4.19",
    "marked": "^4.1.1",
    "ng2-charts": "^3.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.6",
    "@angular/cli": "15.2.6",
    "@angular/compiler-cli": "15.2.7",
    "@types/jasmine": "3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4"
  }
}
