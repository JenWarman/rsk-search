{
  "name": "gui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "ng serve --proxy-config=src/proxy.conf.prod.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-api-client": "rm -rf ./src/app/lib/api-client && api-client-generator -s ./src/assets/swagger.json -o ./src/app/lib/api-client -t all",
    "docker-build": "DOCKER_IMAGE_NAME=rsk-search-gui ../deploy/script/docker-build.sh",
    "docker-run": "docker run -it -p 8080:80 warmans/rsk-search-gui:latest",
    "docker-publish": "DOCKER_IMAGE_NAME=rsk-search-gui ../deploy/script/docker-publish.sh",
    "release": "npm run build && npm run docker-build && npm run docker-publish"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~12.2.14",
    "@angular/compiler": "~12.2.14",
    "@angular/core": "~12.2.14",
    "@angular/forms": "~12.2.14",
    "@angular/platform-browser": "~12.2.14",
    "@angular/platform-browser-dynamic": "~12.2.14",
    "@angular/router": "~12.2.14",
    "@ng-select/ng-select": "~7.4.0",
    "@types/w3c-web-hid": "^1.0.0",
    "@types/w3c-web-usb": "^1.0.4",
    "animate.css": "^4.1.1",
    "api-client-generator": "^4.7.1",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.0",
    "chart.js": "^3.6.2",
    "date-fns": "^2.19.0",
    "ng2-charts": "^3.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "~12.2.14",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
